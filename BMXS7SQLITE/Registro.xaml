<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BMXS7SQLITE.Registro" NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana Registro"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Contraseña"/>

            <Button x:Name="btnRegistrar" Text="REGISTRAR" Clicked="btnRegistrar_Clicked"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>